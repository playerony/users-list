{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@pages/*": [
        "./src/pages/*"
      ],
      "@pages": [
        "./src/pages"
      ],
      "@router/*": [
        "./src/router/*"
      ],
      "@router": [
        "./src/router"
      ],
      "@constants": [
        "./src/constants"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@styles": [
        "./src/styles"
      ],
      "@context/*": [
        "./src/context/*"
      ],
      "@context": [
        "./src/context"
      ],
      "@parts/*": [
        "./src/parts/*"
      ],
      "@parts": [
        "./src/parts"
      ],
      "@layout/*": [
        "./src/layout/*"
      ],
      "@layout": [
        "./src/layout"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@utils": [
        "./src/utils"
      ],
      "@hooks/*": [
        "./src/utils/hooks/*"
      ],
      "@hooks": [
        "./src/utils/hooks"
      ],
      "@interfaces/*": [
        "./src/interfaces/*"
